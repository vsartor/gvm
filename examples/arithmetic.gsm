
; Simple integer arithmetic test
; Outputs should always be 50

; Addition
const   -14     r1
const   64      r2
add     r1      r2
show    r2

; Subtraction
const   14      r1
const   64      r2
sub     r1      r2
show    r2

; Multiplication
const   -10     r1
const   -5      r2
mul     r1      r2
show    r2

; Division
const   5       r1
const   250     r2
div     r1      r2
show    r2

; Remainder
const   80      r1
const   130     r2
rem     r1      r2
show    r2

halt
